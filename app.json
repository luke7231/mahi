{
  "expo": {
    "name": "마감히어로",
    "slug": "mahi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mahi-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/mahi-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1562fc"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.luke7299.mahi"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foreground-img.png",
        "backgroundColor": "#1562fc"
      },
      "package": "com.yourcompany.mahi",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a6a28559-6c1b-43a5-a75a-7013d1e2fceb"
      }
    },
    "owner": "luke7299"
  }
}
